{
  "name": "rbx-dress-to-impress-clone",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Packages": {
        "$path": "Packages"
      },
      "RemoteEvents": {
        "$path": "RemoteEvents"
      },
      "RemoteFunctions": {
        "$path": "RemoteFunctions"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$path":"src/StarterPlayer",
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "World": {
        "$className": "Folder",
        "$path": "./World"
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}